<p id="notice"><%= notice %></p>

<h1>Listing Timelines</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Game</th>
      <th>Entry</th>
      <th>Entry time</th>
      <th>Entry user</th>
      <th>Tags</th>
      <th>Reply to</th>
      <th>Text</th>
      <th>Retweet flag</th>
      <th>Favorite flag</th>
      <th>Warning flag</th>
      <th>Delete flag</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timelines.each do |timeline| %>
      <tr>
        <td><%= timeline.user_id %></td>
        <td><%= timeline.game_id %></td>
        <td><%= timeline.entry_id %></td>
        <td><%= timeline.entry_time %></td>
        <td><%= timeline.entry_user_id %></td>
        <td><%= timeline.tags %></td>
        <td><%= timeline.reply_to %></td>
        <td><%= timeline.retweet_flag %></td>
        <td><%= timeline.favorite_flag %></td>
        <td><%= timeline.warning_flag %></td>
        <td><%= timeline.delete_flag %></td>
        <td><%= timeline.created_at %></td>
        <td><%= timeline.updated_at %></td>
        <td><%= link_to 'Show', timeline %></td>
        <td><%= link_to 'Edit', edit_timeline_path(timeline) %></td>
        <td><%= link_to 'Destroy', timeline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeline', new_timeline_path %>
