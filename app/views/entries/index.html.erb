<p id="notice"><%= notice %></p>

<h1>Listing Entries</h1>

<%= link_to 'New Entry', new_entry_path %>
<br />
<table>
  <thead>
    <tr>
      <th>Entry</th>
      <th>Entry time</th>
      <th>Entry user</th>
      <th>Game</th>
      <th>Content</th>
      <th>Tags</th>
      <th>Reply to</th>
      <th>Entry type</th>
      <th>Parent</th>
      <th>Relation tag</th>
<!--
      <th>Image path01</th>
      <th>Image path02</th>
      <th>Image path03</th>
      <th>Image path04</th>
-->
      <th>Retweet num</th>
      <th>Favorite num</th>
      <th>Warning num</th>
      <th>Delete flag</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.entry_id %></td>
        <td><%= entry.entry_time %></td>
        <td><%= entry.entry_user_id %></td>
        <td><%= entry.game_id %></td>
        <td><%= entry.content %></td>
        <td><%= entry.entry_tags_ids %></td>
        <td><%= entry.reply_to %></td>
        <td><%= entry.entry_type %></td>
        <td><%= entry.parent_id %></td>
        <td><%= entry.relation_tag %></td>
<!--
        <td><%= entry.image_path01 %></td>
        <td><%= entry.image_path02 %></td>
        <td><%= entry.image_path03 %></td>
        <td><%= entry.image_path04 %></td>
        <td><%= entry.retweet_num %></td>
        <td><%= entry.favorite_num %></td>
        <td><%= entry.warning_num %></td>
        <td><%= entry.delete_flag %></td>
-->
        <td><%= entry.created_at %></td>
        <td><%= entry.updated_at %></td>
        <td><%= link_to 'Show', entry %></td>
        <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
        <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_entry_path %>
