<p id="notice"><%= notice %></p>

<h1>Listing Games</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Name</th>
      <th>Description</th>
      <th>Tag name</th>
      <th>Icon image path</th>
      <th>Header image path</th>
      <th>Web view url</th>
      <th>Profile key name01</th>
      <th>Profile key name02</th>
      <th>Profile key name03</th>
      <th>Profile key name04</th>
      <th>Profile key name05</th>
      <th>Profile key name06</th>
      <th>Profile key name07</th>
      <th>Profile key name08</th>
      <th>Profile key name09</th>
      <th>Profile key name10</th>
      <th>Play start date</th>
      <th>Login start time</th>
      <th>Login finish time</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.game_id %></td>
        <td><%= game.name %></td>
        <td><%= game.description %></td>
        <td><%= game.tag_name %></td>
        <td><%= game.icon_image_path %></td>
        <td><%= game.header_image_path %></td>
        <td><%= game.web_view_url %></td>
        <td><%= game.profile_key_name01 %></td>
        <td><%= game.profile_key_name02 %></td>
        <td><%= game.profile_key_name03 %></td>
        <td><%= game.profile_key_name04 %></td>
        <td><%= game.profile_key_name05 %></td>
        <td><%= game.profile_key_name06 %></td>
        <td><%= game.profile_key_name07 %></td>
        <td><%= game.profile_key_name08 %></td>
        <td><%= game.profile_key_name09 %></td>
        <td><%= game.profile_key_name10 %></td>
        <td><%= game.play_start_date %></td>
        <td><%= game.login_start_time %></td>
        <td><%= game.login_finish_time %></td>
        <td><%= game.created_at %></td>
        <td><%= game.updated_at %></td>
        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Edit', edit_game_path(game) %></td>
        <td><%= link_to 'Destroy', game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

